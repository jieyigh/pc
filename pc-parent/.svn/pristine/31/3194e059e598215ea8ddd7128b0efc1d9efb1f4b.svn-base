package com.ds.statements.statements.ssc;

/**
 * ssc下注号码顺序
 * 
 * @author jackson
 *
 */
public enum OrderBetNum {

	TEN_THOUSAND(1, "万号位"), //
	THOUSAND(2, "千号位"), //
	HUNDRED(3, "百号位"), //
	TEN(4, "十号位"), //
	UNIT(5, "个号位"), //
	TOTAL(6, "总和位");

	private Integer orderBetNum;
	private String orderBetName;

	private OrderBetNum(Integer orderBetNum, String orderBetName) {
		this.orderBetNum = orderBetNum;
		this.orderBetName = orderBetName;
	}

	public Integer getOrderBetNum() {
		return orderBetNum;
	}

	public String getOrderBetName() {
		return orderBetName;
	}

}
