package com.ds.transfer.vo;

import java.math.BigDecimal;

public class MoneyTransferVo {

	private Integer siteId;

	private Integer tableId;

	private String username;

	private String orginalUsername;

	private String type; // 转账类型 : IN 转进  OUT 转出

	private BigDecimal money; // 转账金额

	private String billno;

	private String memo;

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getOrginalUsername() {
		return orginalUsername;
	}

	public void setOrginalUsername(String orginalUsername) {
		this.orginalUsername = orginalUsername;
	}

	public Integer getTableId() {
		return tableId;
	}

	public void setTableId(Integer tableId) {
		this.tableId = tableId;
	}

	public String getBillno() {
		return billno;
	}

	public void setBillno(String billno) {
		this.billno = billno;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Integer getSiteId() {
		return siteId;
	}

	public void setSiteId(Integer siteId) {
		this.siteId = siteId;
	}

	public BigDecimal getMoney() {
		return money;
	}

	public void setMoney(BigDecimal money) {
		this.money = money;
	}

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

}
